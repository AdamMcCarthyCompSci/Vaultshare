{
  "name": "splitwisebackend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "babel ./src --out-dir build",
    "start": "node ./build/bin/www",
    "startdev": "nodemon --exec babel-node ./src/bin/www",
    "lint": "./node_modules/.bin/eslint ./src",
    "pretty": "prettier --write '**/*.{js,json}' '!node_modules/**'",
    "postpretty": "yarn lint --fix",
    "createAll": "babel-node ./src/utils/queries/all/create",
    "populateAll": "babel-node ./src/utils/queries/all/populate",
    "dropAll": "babel-node ./src/utils/queries/all/drop",
    "createMembers": "babel-node ./src/utils/queries/members/create",
    "populateMembers": "babel-node ./src/utils/queries/members/populate",
    "dropMembers": "babel-node ./src/utils/queries/members/drop",
    "createGroups": "babel-node ./src/utils/queries/groups/create",
    "populateGroups": "babel-node ./src/utils/queries/groups/populate",
    "dropGroups": "babel-node ./src/utils/queries/groups/drop",
    "createGroupMembers": "babel-node ./src/utils/queries/groupMembers/create",
    "populateGroupMembers": "babel-node ./src/utils/queries/groupMembers/populate",
    "dropGroupMembers": "babel-node ./src/utils/queries/groupMembers/drop",
    "createExpenses": "babel-node ./src/utils/queries/expenses/create",
    "populateExpenses": "babel-node ./src/utils/queries/expenses/populate",
    "dropExpenses": "babel-node ./src/utils/queries/expenses/drop",
    "createSplits": "babel-node ./src/utils/queries/splits/create",
    "populateSplits": "babel-node ./src/utils/queries/splits/populate",
    "dropSplits": "babel-node ./src/utils/queries/splits/drop",
    "createFriends": "babel-node ./src/utils/queries/friends/create",
    "populateFriends": "babel-node ./src/utils/queries/friends/populate",
    "dropFriends": "babel-node ./src/utils/queries/friends/drop"
  },
  "dependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/node": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/register": "^7.15.3",
    "@babel/runtime": "^7.15.4",
    "axios": "^0.21.4",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "express": "~4.16.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.3",
    "method-override": "^3.0.0",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.12",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "pg-promise": "^10.11.0",
    "prettier": "^2.4.1",
    "save": "^2.4.0",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "kill-port": "^1.6.1"
  }
}
